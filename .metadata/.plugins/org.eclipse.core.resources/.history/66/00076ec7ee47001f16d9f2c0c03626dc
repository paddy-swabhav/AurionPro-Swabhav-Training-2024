package com.techlabs.list.test;

import java.util.ArrayList;

public class ArrayListDemo {

	public static void main(String[] args) {
		ArrayList list = new ArrayList();
		System.out.println(list);
		System.out.println(list.size());
		
		list.add("paddy");
		list.add("sova");
		
		System.out.println(list);
	}


}
