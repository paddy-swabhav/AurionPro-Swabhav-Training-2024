package com.techlabs.test;

public class TicTacToe {

	public static void main(String[] args) {
		String arr[][] = new String[3][3];
		
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<3;j++)
			{
				arr[i][j]="x";
			}
		}
		
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<3;j++)
			{
				if(j==1)
					System.out.print("|");
				System.out.print("\t"+arr[i][j]);
				if(j==1)
					System.out.print("|");
			}
			System.out.println("\n\t-----------------");
		}

	}

}
