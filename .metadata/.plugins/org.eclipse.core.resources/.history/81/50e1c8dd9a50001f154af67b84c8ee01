package com.techlabs.state.model;

public class Packet {
	IPacketState state;
	
	public Packet(IPacketState state) {
		// TODO Auto-generated constructor stub
	}

	public void nextState()
	{
		state.next(this);
	}
	
	public void previousState()
	{
		state.previous(this);;
	}
	
	public void printStatus()
	{
		state.current(this);;
	}

}
