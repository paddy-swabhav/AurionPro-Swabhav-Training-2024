package com.techlabs.creational.factory.model;

public class AccountCreater {

	public static IAccount createAcoount(AccountType accountType)
	{
		IAccount account= null;
		if(accountType.equals(AccountType.Current))
			account = new CurrentAccount(101,10000);
		if(accountType.equals(AccountType.Savings))
			account = new SavingsAccount(201,20000);
		
		return account;
	}
	
}
