package com.techlabs.creational.factory.model;

public class CarFactory {
	
	public Icar makeCar(String carName)
	{
		Icar car = null;
		
		if(carName=="Maruti")
			car = new Maruti();
		if(carName=="Tata")
			car = new Tata();
		if(carName=="Mahindra")
			car = new Mahindra();
		
		return car;
	
	}
}
